{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "NativeCLI",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "integrations": {
    "fathom": {
      "siteId": "UOLVXIIC"
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "getting-started/installation",
              "getting-started/start-new-project"
            ]
          },
          {
            "group": "Commands",
            "pages": [
              "commands/new",
              "commands/build",
              "commands/cache-clear",
              "commands/check-update",
              "commands/inertia-fix",
              "commands/ios-install",
              "commands/jump",
              "commands/mobile-install",
              "commands/mobile-upgrade",
              "commands/plugin-add",
              "commands/plugin-list",
              "commands/plugin-remove",
              "commands/plugin-provider",
              "commands/plugin-permissions",
              "commands/self-update",
              "commands/update"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/starter-kits",
              "guides/configuration"
            ]
          },
          {
            "group": "Community",
            "pages": [
              "community/contributing",
              "community/sponsoring"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "NativePHP",
          "href": "https://nativephp.com",
          "icon": "code"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/NativeCLI/nativecli",
          "icon": "github"
        },
        {
          "anchor": "Sponsor",
          "href": "https://github.com/sponsors/PeteBishwhip",
          "icon": "heart"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Discussions",
        "href": "https://github.com/NativeCLI/nativecli/discussions"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://docs.nativecli.com/getting-started/installation"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/NativeCLI/nativecli"
    }
  }
}
